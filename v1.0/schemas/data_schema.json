{
  "id": "https://w3id.org/mircat/miacme/schema/data_schema.json",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title" : "MIACME data schema",
  "version" : "1.0",
  "description" : "JSON-schema representing an investigation in MIACME",
  "type" : "object",
  "properties" : {
     "@context": {
      "description": "The JSON-LD context",
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "object"
        },
        {
          "type": "array"
        }
      ]
    },
    "@id": {
      "description": "The JSON-LD identifier",
      "type": "string",
      "format": "uri"
    },
    "@type": {
      "description": "The JSON-LD type",
      "type": "string",
      "enum": [
        "Data"
      ]
    },
    "rawImages": {
      "type": "string"
    },
    "processedImages": {
      "type": "string"
    },
    "extractedFeatures": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}